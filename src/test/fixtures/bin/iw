#!/usr/bin/env bash

if [[ $1 == "list" ]]; then
    # TODO: this apparently works
    cat "$(dirname $0)/iw-list.out"
else
    if [[ $2 == "phy23" ]]; then
        if [[ ${MOCK_IW_80211N} == "FALSE" ]]; then
            cat "$(dirname $0)/iw-info-false.out"
        else
            cat "$(dirname $0)/iw-info-true.out"
        fi
    fi
fi